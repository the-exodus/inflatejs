{
  "name": "inflatejs",
  "version": "1.0.0",
  "description": "A tool to unminify JavaScript code and make it more readable",
  "main": "dist/index.js",
  "bin": {
    "inflatejs": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2"
  },
  "keywords": [
    "javascript",
    "minify",
    "unminify",
    "beautify",
    "format"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.13.10",
    "jest": "^30.2.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@babel/generator": "^7.26.10",
    "@babel/parser": "^7.26.10",
    "@babel/traverse": "^7.26.10",
    "@babel/types": "^7.26.10"
  }
}
